<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>   
</head>

<body>

  <% include ../partials/nav.ejs %>

<div class="jumbotron text-center">
    <h1>Tournament Structure</h1>
    <hr class="jb-header-hr long-hr"/>
</div>  
<div class="main-wrapper text-center">
    <h1>WylieWoo's Tournament</h1>
    <p class="jb-txt mb-3">Starting Chips - 1500</p>
    <table class="table table-borderless">
        <thead class="thead-dark">
          <tr>
            <th scope="col">Time</th>
            <th scope="col">Small Blind</th>
            <th scope="col">Big Blind</th>
            <th scope="col">Ante</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row">0m</th>
            <td>5</td>
            <td>10</td>
            <td>-</td>
          </tr>
          <tr>
            <th scope="row">30m</th>
            <td>10</td>
            <td>20</td>
            <td>-</td>
          </tr>
          <tr>
            <th scope="row">1hr</th>
            <td>15</td>
            <td>30</td>
            <td>-</td>
          </tr>
          <tr>
            <th scope="row">1hr 30m</th>
            <td>25</td>
            <td>50</td>
            <td>10</td>
          </tr>
          <tr>
            <th scope="row">2hr</th>
            <td>50</td>
            <td>100</td>
            <td>20</td>
          </tr>
          <tr>
            <th scope="row">2hr 30m</th>
            <td>80</td>
            <td>160</td>
            <td>30</td>
          </tr>
          <tr>
            <th scope="row">3hr</th>
            <td>120</td>
            <td>240</td>
            <td>50</td>
          </tr>
          <tr>
            <th scope="row">3hr 30m</th>
            <td>200</td>
            <td>400</td>
            <td>100</td>
          </tr>
          <tr>
            <th scope="row">4hr</th>
            <td>500</td>
            <td>1000</td>
            <td>250</td>
          </tr>
        </tbody>
      </table>
</div>  
    <% include ../partials/scripts.ejs %>
</body>
</html>
